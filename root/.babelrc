{
    "presets": [
        ["es2015", { "modules": false }],
        "es2016",
        "es2017"
    ],
    "plugins": [
        "transform-flow-strip-types",
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }]
    ],
    "env": {
        "test": {
            "presets": [
                "es2015",
                "es2016",
                "es2017"
            ]
        }
    }
}